CREATE DATABASE IF NOT EXISTS AULA02;

USE AULA02;

CREATE TABLE clientes(
id int auto_increment primary key,
nome varchar (80),
cidade varchar (80)
);

INSERT clientes(nome, cidade) VALUES
('Caio', 'Guarulhos'),
('Pedro', 'São Paulo'),
('Fernando', 'Mairiporã'),
('Gabriel', 'Mairiporã'),
('João', 'Mairiporã');



